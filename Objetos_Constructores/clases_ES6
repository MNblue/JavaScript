        this.color = color;
        this.patas = patas;
    }

    saludar() {
        console.log('Â¡hola, soy ' + this.nombre + '!');
    }
}

let gatito = new Mascota('Miki', 9, 'gris', 4);

console.log(gatito.nombre);
